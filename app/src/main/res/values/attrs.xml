<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ChooseTab">
        <attr name="leftSelectDrawable" format="dimension"/>
        <attr name="leftUnSelectDrawable" format="dimension"/>
        <attr name="rightSelectDrawable" format="dimension"/>
        <attr name="rightUnSelectDrawable" format="dimension"/>
        <attr name="textSize" format="dimension"/>
        <attr name="selectTextColor" format="color"/>
        <attr name="unSelectTextColor" format="color"/>
        <attr name="selectText" format="string"/>
        <attr name="unSelectText" format="string"/>
        <attr name="gap" format="dimension"/>
        <attr name="selectSide" format="boolean"/>

    </declare-styleable>
    <declare-styleable name="BreathView">
        <attr name="breathDrawable" format="dimension"/>
        <attr name="bigCircleRadius" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="RollingText">
        <attr name="rollTextSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="TabItemView">
        <attr name="item_icon" format="reference"/>
        <attr name="item_color" format="color"/>
        <attr name="text" format="string"/>
        <attr name="text_size" format="dimension"/>
    </declare-styleable>
</resources>