<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ChooseTab">
        <attr name="leftSelectDrawable" format="dimension"/>
        <attr name="leftUnSelectDrawable" format="dimension"/>
        <attr name="rightSelectDrawable" format="dimension"/>
        <attr name="rightUnSelectDrawable" format="dimension"/>
        <attr name="textSize" format="dimension"/>
        <attr name="selectTextColor" format="color"/>
        <attr name="unSelectTextColor" format="color"/>
        <attr name="selectText" format="string"/>
        <attr name="unSelectText" format="string"/>
        <attr name="gap" format="dimension"/>
        <attr name="selectSide" format="boolean"/>

    </declare-styleable>
    <declare-styleable name="BreathView">
        <attr name="breathDrawable" format="dimension"/>
        <attr name="bigCircleRadius" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="RollingText">
        <attr name="rollTextSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="TabItemView">
        <attr name="item_icon" format="reference"/>
        <attr name="item_color" format="color"/>
        <attr name="text" format="string"/>
        <attr name="text_size" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="BezierSeekBar">
        <attr name="bsBar_color_line" format="reference|color" />
        <attr name="bsBar_color_ball" format="reference|color" />
        <attr name="bsBar_color_value" format="reference|color" />
        <attr name="bsBar_color_value_selected" format="reference|color" />
        <attr name="bsBar_color_bg_selected" format="reference|color" />
        <attr name="bsBar_value_min" format="integer" />
        <attr name="bsBar_value_max" format="integer" />
        <attr name="bsBar_value_selected" format="integer" />
        <attr name="bsBar_unit" format="reference|string" />
    </declare-styleable>
    <declare-styleable name="SelectBar">
        <attr name="sBar_textSize" format="dimension" />
        <!--左文字间距-->
        <attr name="sBar_leftTextDistance" format="dimension" />
        <!--左文字-->
        <attr name="sBar_leftText" format="string" />
        <!--右文字间距-->
        <attr name="sBar_rightTextDistance" format="dimension" />
        <!--右文字-->
        <attr name="sBar_rightText" format="string" />
        <!--选择器个数-->
        <attr name="sBar_circleNum" format="enum">
            <enum name="ONE" value="1" />
            <enum name="TWO" value="2" />
        </attr>
        <!--两个圆之间的间隔数-->
        <attr name="sBar_circleApart" format="integer" />
        <!--<attr name="bsBar_color_value_selected" format="reference|color" />-->
        <!--<attr name="bsBar_color_bg_selected" format="reference|color" />-->
        <!--<attr name="bsBar_value_min" format="integer" />-->
        <!--<attr name="bsBar_value_max" format="integer" />-->
        <!--<attr name="bsBar_value_selected" format="integer" />-->
        <!--<attr name="bsBar_unit" format="reference|string" />-->
    </declare-styleable>
</resources>